<div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">FIN CONTRAT</h3>
        </div>
        <div class="box-body">
                        <table class="table table-striped table-bordered">
                            <tbody>    

                                <tr>
                                    <th>Id</th>
                                    <td>{{ finContrat.id }}</td>            
                                </tr>    

                                <tr>
                                    <th>Observation</th>
                                    <td>{{ finContrat.observation }}</td>            
                                </tr>    
                                <tr>
                                    <th>Code Contrat</th>
                                    <td>{{ finContrat.contrat.code }}</td>            
                                </tr>    
                                <tr>
                                    <th>Code du Bien Choisi</th>
                                    <td>{{ finContrat.contrat.codeBien }}</td>            
                                </tr>    
                                <tr>
                                    <th>Date Creation Du Contrat</th>
                                 <td>{% if finContrat.contrat.createdAt %}{{ finContrat.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>         
                                </tr>    
                            </tbody>
                        </table>
 </div>
</div>